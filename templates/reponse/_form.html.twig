{# templates/form/reponse_form.html.twig #}

{{ form_start(form, {'attr': {'class': 'form', 'novalidate': 'novalidate'}}) }}

<div class="form-group">
    {{ form_label(form.titreRapport) }}
    {{ form_widget(form.titreRapport, {'attr': {'class': 'form-control'}}) }}
    {{ form_errors(form.titreRapport) }}
</div>

<div class="form-group">
    {{ form_label(form.description) }}
    {{ form_widget(form.description, {'attr': {'class': 'form-control'}}) }}
    {{ form_errors(form.description) }}
</div>

<div class="form-group">
    {{ form_label(form.demande) }}
    {{ form_widget(form.demande, {'attr': {'class': 'form-control'}}) }}
    {{ form_errors(form.demande) }}
</div>

<button type="submit" class="btn btn-primary">{{ button_label|default('Save') }}</button>

{{ form_end(form) }}
